<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.my_popular_garage">

    <application
        android:allowBackup="true"
        android:icon="@drawable/welcome"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.My_popular_garage">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.my_popular_garage.Register"
            android:label="@string/app_name" />
        <activity
            android:name="com.example.my_popular_garage.Menu"
            android:label="@string/app_name" />
        <activity
            android:name="com.example.my_popular_garage.Authorization"
            android:label="@string/app_name" />
        <activity
            android:name="com.example.my_popular_garage.Reset_password"
            android:label="@string/app_name" />
    </application>

</manifest>